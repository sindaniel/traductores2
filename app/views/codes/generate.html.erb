<div class="col-lg-12">
  <section class="panel">
    <header class="panel-heading">
     Generar Codigo
    </header>
    <div class="panel-body">

      <%= debug params %>
      <form class="form-horizontal" role="form" method="post">
        <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
        <div class="form-group">
          <label class="col-lg-3 col-sm-3 control-label">Cantidad</label>
          <div class="col-lg-9">
            <div class="iconic-input">
              <i class="fa fa-bullhorn"></i>
              <input type="text" class="form-control" placeholder="Cantitad" name="cantidad">
            </div>
          </div>
        </div>



        <div class="form-group">
          <label class="col-lg-3 col-sm-3 control-label">Minutos</label>
          <div class="col-lg-9">
            <div class="iconic-input">
              <i class="fa fa-bullhorn"></i>
              <input type="text" class="form-control" placeholder="Minutos" name="minutos">
            </div>
          </div>
        </div>


        <button type="submit" class="btn btn-info">Enviar</button>




      </form>
    </div>
  </section>
</div>